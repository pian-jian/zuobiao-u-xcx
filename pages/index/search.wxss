.fixed-searchbar{
    position: fixed;
    top:0;
    z-index: 100;
    width: 100%;
    height: 70rpx;
    background-color: white;
}
.fixed-searchbar .search-bar{
    position: absolute;
    top:0;
    width: 90%;
    height: 70rpx;
    background-color: white;
    border-radius: 10rpx;
}
.search-bar input{
    width:80%;
    margin-left:28rpx ;
}