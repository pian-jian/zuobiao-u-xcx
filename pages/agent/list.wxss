.picker-li{
    width: 25%;
}
.modal-type{
    position: fixed;
    top:0;
    bottom:20;
    left:0;
    right: 0;
    width: 100%;
    height: 100%;
    background-color:rgba(0,0,0,.6);
    z-index: 100;
}
.modal-type .content{
    background-color: white;
    width: 100%;
    padding-bottom: 10rpx;
}
.type-item{
    margin: 14rpx;
}
.m-type .picker-li view{
    border-bottom: 1rpx solid rgba(0,0,0,.2);
    border-right: 1rpx solid rgba(0,0,0,.2);
}
.show-btn{
    height: 80rpx;
    line-height: 80rpx;
}
