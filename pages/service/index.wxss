swiper{
    height: 100%;
    width:100%;
}
.item-image{
    width: 80%;
    height: 100%;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center 80%;
}
.item-image-top1{
    width: 80%;
    height: 100%;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center 20%;
}
.item-bg{
    position: relative;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
}
.icon-anxinchengnuo-arrow{
    position: absolute;

    bottom: 30%;
    display: block;
    overflow: hidden;
    animation: tips_ani 1s ease-out infinite;
}
@-webkit-keyframes tips_ani{0%{-webkit-transform: translateY(20px);opacity: 1;}100%{-webkit-transform: translateY(-10px);opacity: 0;} }
@keyframes tips_ani{0%{transform: translateY(20px);opacity: 1;}100%{transform: translateY(-10px);opacity: 0;} }
